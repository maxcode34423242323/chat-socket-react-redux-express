    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.9.0",
    "gost-transliteration": "^0.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.9",
    "nodemailer": "^6.4.17",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "socket.io": "^3.0.4",
    "socketio-jwt": "^4.6.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6"
